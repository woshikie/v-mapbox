{
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "moduleResolution": "node",
    "lib": [
      "esnext",
      "esnext.asynciterable",
      "dom"
    ],
    "esModuleInterop": true,
    "allowJs": true,
    "sourceMap": true,
    "strict": true,
    "noEmit": true,
    "baseUrl": ".",
    "paths": {
      "~/*": ["./*"],
      "vue": ["@vue/runtime-dom"],
      "@/*": ["./src/*"]
    },
    "types": [
      "@types/node",
      "@types/geojson",
      "@types/mapbox-gl"
    ]
  },
  "include": [
    "src",
  ],
  "exclude": [
    "dist",
    "node_modules"
  ]
}
